{
  "name": "johnfallis/solid",
  "description": "PHP SOLID Principles",
  "keywords": [
    "solid",
    "principles",
    "example"
  ],
  "homepage": "https://johnfallis.com/",
  "require": {
    "php": ">=8.2"
  },
  "config": {
    "platform": {
      "php": "8.2.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.51",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^11",
    "squizlabs/php_codesniffer": "^3.9"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  }
}
